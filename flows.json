    {
        "id": "ed2d399b29970928",
        "type": "function",
        "z": "55a762ae72fa441b",
        "g": "284c27310b25477c",
        "name": "Build MQTT Msg for Window",
        "func": "//--- Basic variables\nvar input = msg.payload;\nvar node_id = msg.payload.bldg;\nvar theWin = msg.payload.window;\nvar theFS = msg.payload.numFire;\n\n//--- Build MQTT message\nif (theFS > 0) {\n    msg.topic = \"nodered/lighton/\" + node_id;\n} else if (theFS > 0) {\n    msg.topic = \"nodered/lighton/\" + node_id;\n}\nmsg.payload = \"{ \\\"whichLight\\\":\" + theWin + \"}\"\n\n//--- Fin\nreturn msg;",
        "outputs": 1,
